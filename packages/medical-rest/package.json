{
  "name": "@jaycorpstudios/medical-rest",
  "version": "0.0.1",
  "author": "Jay <jaycorpstudios@me.com>",
  "main": "./src/index.js",
  "engines": {
    "node": ">=10.15.0",
    "npm": ">=3.0.0",
    "yarn": ">=0.17.9"
  },
  "scripts": {
    "dev": "NODE_ENV=development DEBUG=villafeet-api nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prettier": "prettier --semi false --print-width 120 --single-quote --trailing-comma all --write \"{src,__{tests,mocks}__}/**/*.js\"",
    "test": "NODE_ENV=test DEBUG=villafeet-api jest --runInBand --detectOpenHandles --testTimeout=12000",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "precommit": "lint-staged",
    "seed-local-data": "NODE_ENV=development babel-node src/scripts/seedLocalUsers.js"
  },
  "dependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/node": "7.7.4",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.6",
    "babel-jest": "^24.8.0",
    "body-parser": "1.15.2",
    "cloudinary": "1.15.0",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "^2.4.5",
    "express": "4.14.0",
    "express-session": "^1.15.1",
    "express-validation": "1.0.2",
    "express-winston": "2.1.2",
    "helmet": "3.1.0",
    "http-status": "^0.2.0",
    "joi": "10.2.2",
    "jsonwebtoken": "9.0.0",
    "mailgun-js": "0.22.0",
    "method-override": "^2.3.5",
    "mongoose": "^5.8.0",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "multer-storage-cloudinary": "2.2.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "dotenv-safe": "^8.2.0",
    "husky": "^0.13.2",
    "jest-cli": "24.9.0",
    "lint-staged": "^3.4.0",
    "mockgoose": "8.0.1",
    "nodemon": "^1.11.0",
    "prettier": "1.1.0",
    "supertest": "4.0.2"
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier --semi false --print-width 120 --single-quote --trailing-comma all --write",
      "git add"
    ]
  }
}
